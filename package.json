{
  "private": true,
  "scripts": {
    "check-env": "node ./scripts/node-env.js",
    "jsdoc": "node_modules/.bin/jsdoc -c jsdoc.json -R README.md",
    "doc:dev": "gulp",
    "precommit": "yarn run lint-staged",
    "dev": "umi dev",
    "start": "yarn run check-env && yarn run dev",
    "start:no-mock": "cross-env MOCK=none yarn run start",
    "build": "yarn run check-env && cross-env UMI_ENV=prod umi build",
    "build:zero": "npm run check-env && yarn install && cross-env UMI_ENV=prod umi build",
    "deploy:test": "cross-env DEPLOY_DIST=test node ./scripts/deploy.js",
    "deploy:release": "cross-env DEPLOY_DIST=release node ./scripts/deploy.js",
    "deploy:prod": "cross-env DEPLOY_DIST=prod node ./scripts/deploy.js",
    "deploy:hotfix": "cross-env DEPLOY_DIST=hotfix node ./scripts/deploy.js",
    "deploy:doc": "yarn run jsdoc && cross-env DEPLOY_DIST=doc node ./scripts/deploy.js",
    "analyze": "cross-env ANALYZE=1 umi build",
    "lint-staged": "lint-staged",
    "test": "umi test",
    "test:component": "umi test ./src/components",
    "prettier": "prettier --write './src/**/*'",
    "lint:js": "eslint --fix './src/**/*.{js,jsx}'",
    "lint:ts": "tslint --fix './src/**/*.{ts,tsx}'",
    "lint:less": "stylelint --syntax less './src/**/*.less'",
    "lint": "yarn run lint:js && yarn run lint:ts && yarn run lint:less",
    "e2e:dev": "cypress open",
    "cdoc:dev": "umi-lib doc dev",
    "cdoc:build": "umi-lib doc build"
  },
  "dependencies": {
    "@antv/data-set": "^0.9.6",
    "@babel/runtime": "^7.1.2",
    "ag-grid": "^18.1.2",
    "ag-grid-community": "^20.0.0",
    "ag-grid-enterprise": "^20.0.0",
    "ag-grid-react": "^20.0.0",
    "animate.css": "^3.7.0",
    "antd": "^3.18.1",
    "bignumber.js": "^8.1.1",
    "bizcharts": "^3.2.4",
    "bizcharts-plugin-slider": "^2.0.3",
    "browser-sync": "^2.26.5",
    "classnames": "^2.2.6",
    "d3-scale": "^2.1.2",
    "deep-diff": "^1.0.2",
    "docz": "^1.2.0",
    "dva": "^2.4.0",
    "enquire-js": "^0.2.1",
    "gulp": "^4.0.2",
    "hash.js": "^1.1.5",
    "immer": "^1.7.2",
    "immutability-helper": "^2.8.1",
    "import-all.macro": "^2.0.3",
    "invariant": "^2.2.4",
    "jquery": "^3.3.1",
    "lodash": "^4.17.10",
    "lodash-decorators": "^6.0.0",
    "log-with-style": "^0.3.0",
    "memoize-one": "^4.0.2",
    "mkdirp": "^0.5.1",
    "mobx": "^5.6.0",
    "mobx-react": "^5.3.6",
    "mockjs": "^1.0.1-beta3",
    "module": "^1.2.5",
    "moment": "^2.22.2",
    "numeral": "^2.0.6",
    "nzh": "^1.0.3",
    "omit.js": "^1.0.0",
    "path-to-regexp": "^2.4.0",
    "prop-types": "^15.5.10",
    "qs": "^6.6.0",
    "raw-loader": "^0.5.1",
    "rc-animate": "^2.4.4",
    "rc-texty": "^0.2.0",
    "rc-tween-one": "^2.4.1",
    "react": "^16.8.6",
    "react-container-query": "^0.11.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dnd": "^6.0.0",
    "react-dnd-html5-backend": "^6.0.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.8.6",
    "react-emoji": "^0.5.0",
    "react-fittext": "^1.0.0",
    "react-media": "^1.8.0",
    "react-resizable": "^1.7.5",
    "react-router-dom": "^4.3.1",
    "react-use": "^8.1.3",
    "reselect": "^3.0.1",
    "shelljs": "^0.8.3",
    "sockjs-client": "^1.3.0",
    "stompjs": "^2.3.3",
    "tslint-eslint-rules": "^5.4.0",
    "umi-library": "^1.6.0",
    "uuid": "^3.3.2",
    "ws": "^6.2.1",
    "xlsx": "^0.14.1"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.0.0",
    "@types/d3-scale": "^2.1.0",
    "@types/lodash": "^4.14.118",
    "@types/react": "^16.4.16",
    "@types/react-dom": "^16.0.9",
    "babel-eslint": "^10.0.1",
    "babel-plugin-lodash": "^3.3.4",
    "cross-env": "^5.1.1",
    "cypress": "^3.3.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-compat": "^2.6.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "eslint-plugin-react": "^7.11.1",
    "gh-pages": "^2.0.1",
    "husky": "^1.2.0",
    "jsdoc": "^3.5.5",
    "lint-staged": "^8.1.0",
    "prettier": "1.15.2",
    "stylelint": "^9.8.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.0.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^3.6.0",
    "umi": "^2.2.7",
    "umi-plugin-ga": "^1.1.3",
    "umi-plugin-react": "^1.2.0",
    "webpack-bundle-analyzer": "^2.13.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run precommit"
    }
  },
  "optionalDependencies": {},
  "lint-staged": {
    "linters": {
      "**/*.{js,jsx,ts,tsx,less}": [
        "prettier --write",
        "git add"
      ],
      "**/*.{js,jsx}": "eslint --fix",
      "**/*.{ts,tsx}": "tslint --fix",
      "**/*.less": "stylelint --syntax less"
    },
    "ignore": [
      "**/*.min.*"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "hope-env": {
    "node": "v10.15.3",
    "yarn": "1.16.0"
  }
}
