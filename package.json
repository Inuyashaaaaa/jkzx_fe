{
  "private": true,
  "scripts": {
    "check-env": "node ./scripts/node-env.js",
    "dev": "umi dev",
    "start": "yarn run check-env && yarn run dev",
    "start:no-mock": "cross-env MOCK=none yarn run start",
    "build": "yarn run check-env && cross-env UMI_ENV=prod umi build",
    "build:zero": "npm run check-env && yarn install && cross-env UMI_ENV=prod umi build",
    "deploy:test": "cross-env DEPLOY_DIST=test node ./scripts/deploy.js",
    "deploy:release": "cross-env DEPLOY_DIST=release node ./scripts/deploy.js",
    "deploy:prod": "cross-env DEPLOY_DIST=prod node ./scripts/deploy.js",
    "deploy:hotfix": "cross-env DEPLOY_DIST=hotfix node ./scripts/deploy.js",
    "deploy:feature": "cross-env DEPLOY_DIST=feature node ./scripts/deploy.js",
    "deploy:doc": "cross-env DEPLOY_DIST=doc node ./scripts/deploy.js",
    "deploy:cdoc": "cross-env DEPLOY_DIST=cdoc node ./scripts/deploy.js",
    "deploy:e2e-reports": "cross-env DEPLOY_DIST=e2e-reports node ./scripts/deploy.js",
    "analyze": "cross-env ANALYZE=1 umi build",
    "test": "umi test",
    "test:component": "umi test ./src/components",
    "lint-staged": "lint-staged",
    "lint:js": "eslint --fix './src/**/*.{ts,tsx,js,jsx}'",
    "lint:style": "stylelint --syntax less './src/**/*.less'",
    "prettier": "prettier --write './src/**/*.{js,jsx,ts,tsx,less,css}'",
    "e2e:dev": "cypress open",
    "e2e:build": "cypress run",
    "cdoc:dev": "docz dev",
    "cdoc:build": "docz build",
    "doc:dev": "gulp",
    "doc:build": "node_modules/.bin/jsdoc -c jsdoc.json -R README.md",
    "lint": "yarn run lint:js && yarn run lint:style",
    "precommit": "yarn run lint-staged"
  },
  "dependencies": {
    "@antv/data-set": "~0.9.6",
    "@babel/runtime": "~7.1.2",
    "@commitlint/cli": "~8.0.0",
    "@commitlint/config-angular": "~8.0.0",
    "@umijs/fabric": "~1.0.9",
    "animate.css": "~3.7.0",
    "antd": "~3.19.2",
    "bignumber.js": "~8.1.1",
    "bizcharts": "~3.2.4",
    "bizcharts-plugin-slider": "~2.0.3",
    "browser-sync": "~2.26.5",
    "classnames": "~2.2.6",
    "d3-scale": "~2.1.2",
    "deep-diff": "~1.0.2",
    "docz": "~1.2.0",
    "dva": "~2.4.0",
    "enquire-js": "~0.2.1",
    "gulp": "~4.0.2",
    "hash.js": "~1.1.5",
    "immer": "~1.7.2",
    "immutability-helper": "~2.8.1",
    "import-all.macro": "~2.0.3",
    "invariant": "~2.2.4",
    "jquery": "~3.3.1",
    "jsdoc-babel": "~0.5.0",
    "lodash": "~4.17.10",
    "lodash-decorators": "~6.0.0",
    "log-with-style": "~0.3.0",
    "mathjs": "~6.0.2",
    "memoize-one": "~4.0.2",
    "mkdirp": "~0.5.1",
    "mockjs": "~1.0.1-beta3",
    "module": "~1.2.5",
    "moment": "~2.22.2",
    "nprogress": "~0.2.0",
    "numeral": "~2.0.6",
    "nzh": "~1.0.3",
    "path-to-regexp": "~2.4.0",
    "prop-types": "~15.5.10",
    "qs": "~6.6.0",
    "raw-loader": "~0.5.1",
    "rc-animate": "~2.4.4",
    "rc-texty": "~0.2.0",
    "rc-tween-one": "~2.4.1",
    "react": "~16.8.6",
    "react-container-query": "~0.11.0",
    "react-copy-to-clipboard": "~5.0.1",
    "react-document-title": "~2.0.3",
    "react-dom": "~16.8.6",
    "react-emoji": "~0.5.0",
    "react-fittext": "~1.0.0",
    "react-media": "~1.8.0",
    "react-router-dom": "~4.3.1",
    "react-use": "~8.1.3",
    "reselect": "~3.0.1",
    "shelljs": "~0.8.3",
    "sockjs-client": "~1.3.0",
    "stompjs": "~2.3.3",
    "umi-library": "~1.6.0",
    "uuid": "~3.3.2",
    "ws": "~6.2.1",
    "xlsx": "~0.14.1"
  },
  "devDependencies": {
    "@babel/polyfill": "~7.0.0",
    "@babel/preset-typescript": "~7.3.3",
    "@commitlint/cli": "~8.0.0",
    "@commitlint/config-angular": "~8.0.0",
    "@types/d3-scale": "~2.1.0",
    "@types/lodash": "~4.14.118",
    "@types/react": "~16.4.16",
    "@types/react-dom": "~16.0.9",
    "babel-eslint": "~10.0.1",
    "babel-plugin-import": "~1.11.2",
    "babel-plugin-lodash": "~3.3.4",
    "cross-env": "~5.1.1",
    "css-loader": "~2.1.1",
    "cypress": "~3.3.0",
    "docz-theme-default": "~1.2.0",
    "eslint": "~5.16.0",
    "husky": "~1.2.0",
    "jsdoc": "~3.5.5",
    "less-loader": "~5.0.0",
    "lint-staged": "~8.1.0",
    "mocha": "~5.2.0",
    "mochawesome": "~4.0.1",
    "prettier": "~1.18.2",
    "style-loader": "~0.23.1",
    "stylelint": "~10.1.0",
    "umi": "~2.4.4",
    "umi-plugin-react": "~1.4.2",
    "webpack-bundle-analyzer": "~2.13.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run precommit",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "linters": {
      "**/*.{js,jsx,ts,tsx,less,css}": [
        "prettier --write",
        "git add"
      ],
      "**/*.{js,jsx,ts,tsx}": "eslint --quiet --fix",
      "**/*.less": "stylelint --syntax less --fix"
    },
    "ignore": [
      "**/*.min.*"
    ]
  },
  "optionalDependencies": {},
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "hope-env": {
    "node": "v10.15.3",
    "yarn": "1.16.0"
  }
}
